{% extends 'security/admin.html.twig'%}

{% block title %}Incubator{% endblock %}

{% block content %}
    <h1>Incubator</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ incubator.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ incubator.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ incubator.description }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ incubator.picture }}</td>
            </tr>
            <tr>
                <th>Cover</th>
                <td>{{ incubator.cover }}</td>
            </tr>
            <tr>
                <th>IsPublic</th>
                <td>{{ incubator.isPublic ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('incubator_index') }}">back to list</a>

    <a href="{{ path('incubator_edit', {'id': incubator.id}) }}">edit</a>

    {{ include('admin/incubator/_delete_form.html.twig') }}
{% endblock %}
